{
  "name": "piclist",
  "version": "1.7.12",
  "description": "Modified PicGo core, A tool for picture uploading",
  "author": {
    "name": "Kuingsmile",
    "email": "pkukuing@gmail.com"
  },
  "homepage": "https://piclist.cn",
  "bugs": {
    "url": "https://github.com/Kuingsmile/PicList/issues",
    "email": "pkukuing@gmail.com"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "picgo": "./bin/picgo",
    "picgo-server": "./bin/picgo-server"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "node ./bin/picgo",
    "server": "node ./bin/picgo-server",
    "lint": "eslint src/**/*.ts",
    "build": "cross-env NODE_ENV=production rimraf ./dist && rollup -c rollup.config.js",
    "dev": "cross-env NODE_ENV=development rollup -c rollup.config.js -w",
    "cz": "git-cz",
    "release": "bump-version",
    "lint:dpdm": "dpdm -T --tsconfig ./tsconfig.json --no-tree --no-warning --exit-code circular:1 src/index.ts"
  },
  "keywords": [
    "picture",
    "upload",
    "util"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/node-bump-version/.cz-config.js"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/node-bump-version/commitlint-node"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@types/cross-spawn": "^6.0.5",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/heic-convert": "^1.2.3",
    "@types/image-size": "^0.8.0",
    "@types/inquirer": "^0.0.42",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.14.201",
    "@types/mime": "^3.0.4",
    "@types/mime-types": "^2.1.3",
    "@types/minimatch": "^3.0.3",
    "@types/node": "16.11.7",
    "@types/resolve": "^0.0.8",
    "@types/text-to-svg": "^3.1.4",
    "@types/tunnel": "^0.0.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-eslint": "^10.1.0",
    "builtins": "^4.0.0",
    "conventional-changelog": "^3.0.6",
    "cross-env": "^7.0.3",
    "cz-customizable": "^5.10.0",
    "dpdm": "^3.14.0",
    "eslint": "^8.53.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^1.3.1",
    "node-bump-version": "^1.0.2",
    "pre-commit": "^1.2.2",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.421.0",
    "@aws-sdk/lib-storage": "3.421.0",
    "@aws-sdk/s3-request-presigner": "3.421.0",
    "@picgo/i18n": "^1.0.0",
    "@picgo/store": "^2.1.0",
    "@smithy/node-http-handler": "2.1.6",
    "axios": "^1.6.2",
    "chalk": "^2.4.1",
    "commander": "^8.1.0",
    "cross-spawn": "^7.0.3",
    "dayjs": "^1.11.10",
    "download-git-repo": "^3.0.2",
    "ejs": "^2.6.1",
    "file-type": "16.2.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "globby": "^11.1.0",
    "heic-convert": "^1.2.4",
    "hpagent": "1.2.0",
    "image-size": "^1.0.2",
    "inquirer": "^6.0.0",
    "is-wsl": "^2.2.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mime": "2.5.2",
    "mime-types": "2.1.35",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.8",
    "multer": "^1.4.5-lts.1",
    "node-ssh-no-cpu-features": "^1.0.1",
    "qiniu": "7.9.0",
    "resolve": "^1.8.1",
    "rimraf": "^5.0.1",
    "sharp": "^0.32.1",
    "text-to-svg": "^3.1.5",
    "tunnel": "^0.0.6",
    "uuid": "^9.0.1",
    "webdav": "^4.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kuingsmile/PicList-Core.git"
  },
  "picBed": {
    "current": "smms"
  },
  "plugins": {},
  "engines": {
    "node": ">= 16.0.0"
  }
}
