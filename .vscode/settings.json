{
    "files.trimTrailingWhitespace": true,
    "editor.formatOnSave": false,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.enable": true,
    "editor.tabSize": 2,
    "typescript.tsdk": "node_modules/typescript/lib"
}
